// Place your settings in this file to overwrite default and user settings.
{

    // Configure glob patterns for excluding files and folders.
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/.DS_Store": true,
        "**/.lo": true,
        "**/*.lo": true,
        "**/*.Po": true,
        "**/*.la": true,
        "**/*.o": true,
        "**/Makefile": true,
        "**/Makefile.in": true,
        "**/.libs": true,
        "**/.deps": true,
        "**/m4": true,
        "**/autom4te.cache": true,
        "config.*": true,
        "configure": true,
        "install-sh": true,
        "aclocal.m4": true,
        "compile": true,
        "libtool": true,
        "ltmain.sh": true,
        "missing": true,
        "depcomp": true,
        // Binaries that we generate
        "src/tstools_util": true,
        "src/tstools_pat_inspector": true,
        "src/tstools_pmt_inspector": true,
        "src/tstools_udp_capture": true,
        "src/.gitignore": true,
        "src/tstools_si_inspector": true,
    },
    "files.associations": {
        "__functional_base": "c",
        "__functional_base_03": "c",
        "__tuple": "c",
        "functional": "c",
        "limits": "c",
        "memory": "c",
        "tuple": "c",
        "type_traits": "c",
        "utility": "c",
        "curses.h": "c",
        "math.h": "c",
        "assert.h": "c",
        "stdio.h": "c",
        "psi.h": "c",
        "pmt_inspector.c": "cpp",
        "pat_inspector.c": "cpp",
        "dump.c": "cpp",
        "si_inspector.c": "cpp"
    }
}
